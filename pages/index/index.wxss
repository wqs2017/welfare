
page{
  width: 100%;
  height: 100%;
  overflow: auto;
  position: relative;
  background: #f5f5f9;
}
.page{
  width: 100%;
  height: 100%;
  position: relative;
}
.banenr_box{
  width: 100%;
  position: relative;
  overflow: hidden;
  padding-top: 40rpx;
}
.banner{
  width: 670rpx;
  height: 386rpx;
  margin: 0 auto;
  position: relative;
  box-sizing: border-box;
}
.bottom_border{
  width: 670rpx;
  height: 40rpx;
  margin: 0 auto;
  border-bottom: 1rpx solid #e6e7e8;
}
/* center box */
.center_tip{
  width: 100%;
  height: 40rpx;
  margin-top: 40rpx;
  color: #a1a1a1;
  text-align: center;
}
.center_scroll{
  width: 670rpx;
  height: 140rpx;
  position: relative;
  margin: 30rpx auto 0;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
  opacity: 0;
}
.center_box{
  height: 140rpx;
  position: relative;
  white-space: nowrap;
}
.center_item image,.center_cover{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.center_item{
  width: 210rpx;
  height: 140rpx;
  margin-right: 20rpx;
  float: left;
  position: relative;
}
.center_cover{
  z-index: 2;
  background: rgba(0,0,0,0.3);
}
.center_text{
  width: 100%;
  height: 140rpx;
  line-height: 140rpx;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  position: relative;
  z-index: 3;
}
/*  */
.banner_title0{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 42rpx;
  padding-left: 40rpx;
}
.banner_title1{
  width: 100%;
  height: 33rpx;
  line-height: 33rpx;
  font-size: 24rpx;
  padding-left: 40rpx;
