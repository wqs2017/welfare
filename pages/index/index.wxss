}
.item_triangle{
  width: 0;
  height: 0;
  border-left: 10rpx solid transparent;
  border-right: 10rpx solid transparent;
  border-top: 10rpx solid #fed917;
  position: absolute;
  left: 50%;
  margin-left: -5rpx;
  top: -20rpx;
}

/* 底部  */
.fixed_box{
  min-width: 380rpx;
  height: 100rpx;
  overflow: hidden;
  padding: 30rpx 140rpx 0 96rpx;
  background: rgba(0,0,0,0.68);
  color: #fff;
  position: fixed;
  bottom: 16rpx;
  left: 24rpx;
  border-radius: 100rpx;
  z-index: 9;
}
.user_img{
  width: 72rpx;
  height: 72rpx;
  position: absolute;
  left: 16rpx;
  top: 50%;
  margin-top: -36rpx;
  background: #fff;
  border-radius: 50%;
}
.user_img image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.username{
  min-width: 80rpx;
  max-width: 150rpx;
  float: left;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  height: 40rpx;
  line-height: 40rpx;
  color: #fff;
  position: relative;
}
.circle_tip{
  width: 16rpx;
  height: 16rpx;
  position: absolute;
  right: 0rpx;
  top: 0rpx;
  border-radius: 50%;
  background: #f86566;
}
.username_border{
  width: 40rpx;
  height: 40rpx;
  float: left;
  border-right: 1rpx solid #e6e7e8;
}
.user_cart{
  width: 140rpx;
  height: 100rpx;
  position: absolute;
  right: 0;
  top: 0;
  background: url(http://ozg7zdnzy.bkt.clouddn.com/fls_cart_bg0.png) 40rpx center no-repeat;
  background-size: 46rpx 46rpx;
}
.cart_num{
  height: 28rpx;
  min-width: 28rpx;
  position: absolute;
  left: 84rpx;
  top: 14rpx;
  background: #f29200;
  line-height: 28rpx;
  color: #fff;
  text-align: center;
  padding: 0 6rpx;
  border-radius: 14rpx;
}

.lead_box{
  position: fixed;
  left: 0;
  top: 0;
  overflow: auto;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.68);
  z-index: 9;
}
.lead_out_cover{
  width: 100%;
  height: 100%;
  z-index: 1;
  position: absolute;
  left: 0;
  top: 0;
}
.lead_img{
  margin: 50rpx auto 0;
  width: 580rpx;
  height: 956rpx;
  background: url(http://ozg7zdnzy.bkt.clouddn.com/fls_new_user.png) center center no-repeat;
  background-size: 100% 100%;
  position: relative;
  z-index: 9;
}
.lead_btn{
  width: 320rpx;
  height: 94rpx;
  position: absolute;
  left: 50%;
  margin-left: -160rpx;
  bottom: 100rpx;
  text-align: center;
  border-radius: 100rpx;
  line-height: 94rpx;
  font-size: 36rpx;
  background: #fff;
  color: #f29200;
  font-weight: 600;
}
.lead_cover{
  width: 100%;
  height: 156rpx;
  position: absolute;
  left: 0;
  top: 0;
}

/* card alert  */
.alert_box{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0,0,0,0.7);
  z-index: 10;
}
.alert_cover{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}
.alert_card_box{
  width: 690rpx;
  overflow: hidden;
  min-height: 750rpx;
  position: absolute;
  left: 50%;
  margin-left: -345rpx;
  top: 30rpx;
  background: #fff;
  padding: 10rpx;
  box-shadow: 0 0 20rpx 0 rgba(0,0,0,0.16);
  border-radius: 8rpx;
  z-index: 9;
}
.card_box{
  width: 670rpx;
  height: 335rpx;
  margin: 0 auto;
  position: relative;
  background: url(http://ozg7zdnzy.bkt.clouddn.com/fls_bg_card.jpg) center center no-repeat;
  background-size: 100% 100%;
  border-radius: 8rpx;
  box-shadow: 0 0 20rpx 0 rgba(242,146,0,0.16);
}
.card_name{
  width: 100%;
  height: 66rpx;
  line-height: 66rpx;
  color: #fff;
  font-size: 48rpx;
  font-weight: 600;
  padding-left: 30rpx;
  position: absolute;
  left: 0;
  top: 30rpx;
}
.card_id{
  width: 100%;
  padding-left: 30rpx;
  color: #fff;
  font-size: 32rpx;
  line-height: 45rpx;
  height: 45rpx;
  position: absolute;
  left: 0;
  top: 105rpx;
}
.card_price{
  width: 100%;
  height: 118rpx;
  position: absolute;
  top: 122rpx;
  right: 0;
  padding-right: 30rpx;
  color: #fff;
  line-height: 118rpx;
  font-size: 84rpx;
  font-weight: 600;
  text-align: right;
}
.card_price text{
  font-size: 48rpx;
  line-height: 134rpx;
  color: #fff;
}
.card_old_price{
  width: 100%;
  padding-right: 30rpx;
  font-size: 56rpx;
  color: #FFD391;
  text-decoration: line-through;
  position: absolute;
  right: 0;
  top: 227rpx;
  text-align: right;
}
.a_tip_box{
  width: 670rpx;
  overflow: hidden;
  position: relative;
  border: 2rpx solid #e6e7e8; 
  padding: 30rpx;
  margin-top: 30rpx;
}
.a_tip_title{
  width: 100%;
  height: 48rpx;
  line-height: 48rpx;
  font-size: 36rpx;
  font-weight: 600;
  text-align: center;
}
.a_tip_text{
  width: 100%;
  line-height: 50rpx;
  margin-top: 24rpx;
  font-size: 30rpx;
}
.a_tip_user{
  width: 100%;
  height: 64rpx;
  line-height: 64rpx;
  position: relative;
  margin-top: 30rpx;
}
.a_tip_name{
  height: 64rpx;
  line-height: 64rpx;
  float: right;
  font-size: 30rpx;
}
.a_tip_img{
  width: 64rpx;
  height: 64rpx;
  float: right;
  margin-right: 10rpx;
  background: #ccc;
  border-radius: 50%;
  position: relative;
}
.a_tip_img image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.card_buy{
  width: 374rpx;
  height: 94rpx;
  line-height: 94rpx;
  color: #fff;
  font-size: 36rpx;
  background: #f29200;
  text-align: center;
  border-radius: 8rpx;
  margin: 30rpx auto 20rpx;
}
