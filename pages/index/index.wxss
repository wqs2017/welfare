  background-size: 460rpx 8rpx;
}
.item_out_box{
  width: 100%;
  overflow: hidden;
  position: relative;
  padding: 0 20rpx 40rpx 40rpx;
}
.item_box{
  width: 325rpx;
  height: 488rpx;
  margin-right: 20rpx; 
