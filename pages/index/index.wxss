.item_circle view{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.item_circle_bg0{
  background: #d8d8d8;
}
.item_circle_bg1{
  background: #fed917;
}
.item_head{
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  top: 0;
