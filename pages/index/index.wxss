  white-space: nowrap;
}
.i_text0{
  width: 100%;
  height: 45rpx;
  line-height: 45rpx;
  margin-top: 20rpx;
  font-size: 32rpx;
  font-weight: 600;
}
.i_text1{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  color: #a1a1a1;
